module module1; 

require document with columns
  id Integer
  and number Integer
  and company Text
  and make Text
  and model Text
  and year Integer
  and area Text
  and city Text
  and state Text
  and vin Text
  and comments Text;
  
  
create view ID as select Document.id as match from Document;
 
output view ID;

create view COMMENTS as select Document.comments as match from Document;
 
output view COMMENTS;
