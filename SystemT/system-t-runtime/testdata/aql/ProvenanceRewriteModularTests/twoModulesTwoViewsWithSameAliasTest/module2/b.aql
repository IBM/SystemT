module module2;

import view CapsWord from module module1 as CapsWord1;

create view First as
extract regex /[A-Z][a-z]+(\-[A-Z][a-z]+)?/ on D.text as match
from Document D;

create view Person1 as
select CombineSpans(F.match, C.match) as name
from First F, CapsWord1 C
where FollowsTok(F.match, C.match, 0, 0);

output view Person1;