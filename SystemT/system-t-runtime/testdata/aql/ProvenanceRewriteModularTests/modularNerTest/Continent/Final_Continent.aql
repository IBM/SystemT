module Continent;

create view Continent as
select 
	--C.match as match, GetText(C.match) as match_text, GetText(C.match_detag) as match_detag_text
	C.match_detag as continent
from
(
	select 
		--Remap(C.reference) as match, 
		C.reference as match_detag	
	from Continent_ C
) C
;


-- Export final views
export view Continents;
export view Continent_;
export view Continent;
