create view AllCapsOrg as
	select 
		R.match as match
	from 
	-- to capture ABC
	--	Regex(/\b(\p{Lu}\p{M}*){2,}\b/, Doc.text) R;
	-- allow matches for B&B, BB&T
	--	Regex(/\b(\p{Lu}\p{M}*){1,2}(\s*&\s*)?(\p{Lu}\p{M}*)+\b/, Doc.text) R
