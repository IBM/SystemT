-- view containing word + its corresponding polarity
-- these separate views return "negative" if 'fine' is a noun, and "positive" if 'fine' is tagged ADJ.
create view NounsPlusPolarity as
  select NN.nn as word, WTP.polarity as polarity, 'Noun' as partOfSpeech, NN.detailedPos
from AllSentimentNouns NN, WordToPolarityTable_Nouns WTP
where Equals(GetText(NN.nn), GetText(WTP.word));
--output view NounsPlusPolarity;

create view AdjectivesPlusPolarity as
  select JJ.jj as word, WTP.polarity as polarity, 'Adjective' as partOfSpeech, JJ.detailedPos
 from AllSentimentAdjectives JJ, WordToPolarityTable_Adjectives WTP
 where Equals(GetText(JJ.jj), GetText(WTP.word));

create view VerbsPlusPolarity as
  select VB.vb as word, WTP.polarity as polarity, 'Verb' as partOfSpeech, VB.detailedPos
 from AllSentimentVerbs VB, WordToPolarityTable_Verbs WTP
 where Equals(GetText(VB.vb), GetText(WTP.word));

create view AdverbsPlusPolarity as
  select RB.rb as word, WTP.polarity as polarity, 'Adverb' as partOfSpeech, RB.detailedPos
 from AllSentimentAdverbs RB, WordToPolarityTable_Adverbs WTP
 where Equals(GetText(RB.rb), GetText(WTP.word));


