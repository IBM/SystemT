create view TwoDigitsInterval as
select
	R.match as match
from
    RegexTok(/((0)|(1)|(2)|(3))?[\d]( )*[\-]( )*((0)|(1)|(2)|(3))?[\d]/, 5, Doc.text) R;

create view WKDay as
	select
		D.match as match
	from
		Dictionary('dicts/wkday.dict', Doc.text) D;
		
create view TimeZone as
	select
		D.match as match
	from
		Dictionary('dicts/timeZone.dict', Doc.text) D;

