create view TwoDigitsInterval as
select
	R.match as match
from
    RegexTok(/((0)|(1)|(2)|(3))?[\d]( )*[\-]( )*((0)|(1)|(2)|(3))?[\d]/, 5, DocScan.text) R;

create view WKDay as
	select
		D.match as match
	from
		Dictionary('wkday.dict', DocScan.text) D;
		
create view TimeZone as
	select
		D.match as match
	from
		Dictionary('timeZone.dict', DocScan.text) D;

