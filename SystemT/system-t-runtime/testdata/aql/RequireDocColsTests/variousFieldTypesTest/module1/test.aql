module module1; 

require document with columns
  id Integer
  and text Text
  and percentage Float
  and isValid Boolean;
  
 create view ID as select Document.id as match from Document;
 output view ID;
 
 create view TEXT as select Document.text as match from Document;
 output view TEXT;
 
 create view PERCENTAGE as select Document.percentage as match from Document;
 output view PERCENTAGE;
 
 create view ISVALID as select Document.isValid as match from Document;
 output view ISVALID;